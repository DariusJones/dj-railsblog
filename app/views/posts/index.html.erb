<h1>This is the list of all the post</h1>

<table border = "1" >

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <p><td><%= post.content %></td></p>
        <td><a href="/posts/<%= post.id%>">Show</a></td>
        <td><%= link_to "Edit", edit_post_path(post)%></td>
        <td><%= link_to "Delete Post", post_path(post.id), method: :delete%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New Posts', new_post_path %>
